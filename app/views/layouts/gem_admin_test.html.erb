<!DOCTYPE html>
<html>
<head>
  <title>Admin Page</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<h1>Menu</h1>

<ul>
   <% @menu.each do |item| %>
      <li><a href="<%= item[:url] %>"><%= item[:text] %></a></li>
   <% end %>
</ul>

<%= yield %>

</body>
</html>
<